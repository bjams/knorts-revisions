    "759456ed-0b8d-40c7-a954-8560cbc1275e": {
      "type": "rich-text",
      "blocks": {
        "template--14510215069759__759456ed-0b8d-40c7-a954-8560cbc1275e-1668051325a13adde3-0": {
          "type": "heading",
          "settings": {
            "heading": "<p>Knorts Stretch Levels<\/p>"
          }
        },
        "template--14510215069759__759456ed-0b8d-40c7-a954-8560cbc1275e-1668051325a13adde3-1": {
          "type": "text",
          "settings": {
            "text": "<p>Different fabrics have different levels of stretch which affect how they fit our customers.  Here is a guide to help you understand what would work best for you.<\/p>"
          }
        }
      },
      "block_order": [
        "template--14510215069759__759456ed-0b8d-40c7-a954-8560cbc1275e-1668051325a13adde3-0",
